@model List<Department>
@{
    ViewData["Title"] = "Department Page";
}
<h1 class="text-center m-5">@ViewBag.name</h1>
<table class="table table-bordered table-hover w-75 " style="margin: auto;">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Manger Name</th>
    </tr>
    @foreach(var item in (List<Department>) ViewData["data"])
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.manger</td>
        </tr>
    }
</table>


